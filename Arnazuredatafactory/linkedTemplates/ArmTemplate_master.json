{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Arnazuredatafactory"
		},
		"ADFSnowflake Linkseevice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADFSnowflake Linkseevice'"
		},
		"AzureBlobStorageLinkedServices_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureBlobStorageLinkedServices'"
		},
		"SnowflakeLinkedServices_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SnowflakeLinkedServices'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Arnazuredatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADFSnowflake Linkseevice_connectionString": {
						"value": "[parameters('ADFSnowflake Linkseevice_connectionString')]"
					},
					"AzureBlobStorageLinkedServices_sasUri": {
						"value": "[parameters('AzureBlobStorageLinkedServices_sasUri')]"
					},
					"SnowflakeLinkedServices_connectionString": {
						"value": "[parameters('SnowflakeLinkedServices_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}